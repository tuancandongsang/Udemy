<IfModule mod_rewrite.c>
 RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
DirectoryIndex
RewriteEngine On
RewriteBase /
RewriteRule ^(.*)?$ https://mdoweb.com.vn/$1 [P,L]